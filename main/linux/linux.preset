# mkinitcpio preset file for the 'linux@KERNAME@' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-@VERSION@@KERNAME@"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-@VERSION@@KERNAME@.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-@VERSION@@KERNAME@-fallback.img"
fallback_options="-S autodetect"
